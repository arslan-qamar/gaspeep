{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Running Backend (Pick Process)",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${command:pickProcess}"
    },
    {
      "name": "Attach to Delve (Remote :2345)",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "host": "127.0.0.1",
      "port": 2345
    },
    {
      "name": "Launch Backend (dlv debug)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/backend/cmd/api",
      "cwd": "${workspaceFolder}/backend",
      "env": {},
      "args": []
    }
  ]
}
